{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":pinAllExceptPeerDependencies",
    ":disableDigestUpdates"
  ],
  "rebaseWhen": "conflicted",
  "hostRules": [
    {
      "hostType": "git-tags",
      "matchHost": "https://github.com/iSole-3D",
      "encrypted": {
        "token": ""
      }
    },
    {
      "hostType": "docker-compose",
      "matchHost": "https://ghcr.io/iSole-3D",
      "username": "basz",
      "encrypted": {
        "token": ""
      }
    },
    {
      "hostType": "docker",
      "matchHost": "https://ghcr.io/iSole-3D",
      "username": "basz",
      "encrypted": {
        "token": ""
      }
    },
    {
      "hostType": "dockerfile",
      "matchHost": "https://ghcr.io/iSole-3D",
      "username": "basz",
      "encrypted": {
        "token": ""
      }
    }
  ],
  "ignorePaths": [
    "orchestration/device-controller/container-controller/Dockerfile.template"
  ],
  "packageRules": [
    {
      "description": "Automerge non-major updates",
      "matchUpdateTypes": ["minor", "patch"],
      "matchCurrentVersion": "!/^0/",
      "automerge": true
    },
    {
      "packageNames": ["three", "@types/three"],
      "managers": ["npm"],
      "enabled": false
    }
  ]
}
